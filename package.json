  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "koa": "^2.8.2",
    "koa-static": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@comeon/babel-preset-comeon": "^4.0.1",
    "@comeon/prettier-config": "^1.2.1",
    "babel-register": "^6.26.0",
