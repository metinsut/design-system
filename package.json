{
  "name": "@comeon-stockholm/design-system",
  "version": "0.1.0",
  "description": "Saber design system",
  "main": "mod.js",
  "module": "mod.js",
  "files": [
    "dist",
    "mod.js"
  ],
  "exports": {
    "./blitzino/": "./dist/blitzino/",
    "./casinostugan/": "./dist/casinostugan/",
    "./comeon/": "./dist/comeon/",
    "./folkeautomaten/": "./dist/folkeautomaten/",
    "./galaksino/": "./dist/galaksino/",
    "./getlucky/": "./dist/getlucky/",
